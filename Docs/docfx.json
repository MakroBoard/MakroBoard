{
    "build": {
        "content": [{
            "files": [
                "user/**.md",
                "user/**/toc.yml",
                "toc.yml",
                "*.md"
            ]
        }],
        "resource": [{
            "files": [
                "images/**"
            ]
        }],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "statictoc",
            "templates/discordfx",
            "templates/DocFx.Plugins.PlantUml"
            "templates/makroboard"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}